  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lang:containers": "extract-messages -l=pl -d=pl -o src/lang/containers --flat \"src/containers/**/*.messages.js\"",
    "lang:components": "extract-messages -l=pl -d=pl -o src/lang/components --flat \"src/components/**/*.messages.js\"",
    "lang:defaults": "extract-messages -l=pl -d=pl -o src/lang/defaults --flat \"src/utils/default.messages.js\"",
    "lang": "npm run lang:defaults && npm run lang:components && npm run lang:containers"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
